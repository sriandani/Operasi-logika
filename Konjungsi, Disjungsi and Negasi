print ("TABEL KEBENARAN KONJUNGSI, DISJUNGSI DAN  NEGASI")
print("------------------------------------------------- ")

print ("P\tQ\tP AND Q\tP OR Q\t~P or Q")
print("-------------------------------------------------")

for P in [True, False]:
    for Q in [True, False]:
        Konjungsi = P and Q
        Disjungsi = P or Q
        Negasi = not P or Q
        print (f"{P}\t{Q}\t{Konjungsi}\t{Disjungsi}\t{Negasi}")

print("-------------------------------------------------")
